
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


enum Role {
  USER 
  INSTRUCTOR
  ADMIN
}

model User {
  id Int   @id @default(autoincrement())
  name String
  email String  @unique
  password String 
  role Role
}